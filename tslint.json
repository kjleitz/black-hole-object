{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "variable-name": [
      true,
      "check-format",
      "ban-keywords",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "quotemark": false,
    "only-arrow-functions": false
  },
  "rulesDirectory": []
}
